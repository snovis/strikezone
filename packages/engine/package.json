{
  "name": "@strikezone/engine",
  "version": "0.1.0",
  "description": "Core baseball simulation engine with speed and classic modes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "simulate": "npx tsx src/scripts/simulate.ts",
    "test": "echo \"No tests yet\""
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./speed": {
      "types": "./dist/modes/speed/index.d.ts",
      "import": "./dist/modes/speed/index.js"
    },
    "./classic": {
      "types": "./dist/modes/classic/index.d.ts",
      "import": "./dist/modes/classic/index.js"
    }
  },
  "files": ["dist"],
  "devDependencies": {
    "typescript": "^5.3.0",
    "tsx": "^4.7.0"
  },
  "keywords": ["baseball", "simulation", "engine", "dice"]
}
